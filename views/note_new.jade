extends layout

block content
  h1= title
  div.newnote
    form( method="post")
      div
        div
          span.label Title :
          input(type="text", name="title", id="editNoteTitle", autocomplete="off")
        div
          span.label Name :
          input(type="text", name="name", id="editNoteName", 
          autocomplete="off")
        div#editNoteSubmit
          input(class="center" type="submit", value="Save")
    a(href="/")!= "Back to Note List"